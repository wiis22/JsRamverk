{
  "name": "jsramverk",
  "version": "1.0.0",
  "description": "Starter project for DV1677 JSRamverk",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha --exit 'backend/tests/**/*.*js' 'frontend/tests/**/*.*js'",
    "dev": "npm run dev --prefix frontend",
    "build": "npm run build --prefix frontend",
    "startNext": "npm run start --prefix frontend",
    "startExpress": "node ./backend/api_response_handling.mjs",
    "lint": "npm run lint --prefix frontend",
    "start": "concurrently \"npm run startExpress\" \"npm run dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.24.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "mongodb": "^6.9.0",
    "morgan": "^1.10.0",
    "next": "^14.2.13",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.2.1",
    "chai": "^5.1.1",
    "chai-http": "^5.0.0",
    "concurrently": "^9.0.1",
    "mocha": "^10.7.3",
    "nyc": "^17.1.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.12"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "include": [
      "backend/**/*.js",
      "frontend/**/*.js"
    ],
    "exclude": [
      "**/*.test.js"
    ]
  }
}
